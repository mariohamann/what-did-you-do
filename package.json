{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "lint": "npx prettier --write . && php ./vendor/bin/pint",
        "generate-types": "php artisan typescript:transform",
        "// prepare": "We don't want to start the prepare script for husky when we don't install the dev depencies.",
        "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
    },
    "devDependencies": {
        "@inertiajs/vue3": "^1.0.9",
        "@tailwindcss/forms": "^0.5.3",
        "@types/ziggy-js": "^1.3.2",
        "@vitejs/plugin-vue": "^4.2.3",
        "autoprefixer": "^10.4.14",
        "axios": "^1.4.0",
        "husky": "^8.0.3",
        "laravel-vite-plugin": "^0.7.8",
        "lint-staged": "^13.2.3",
        "postcss": "^8.4.24",
        "prettier": "^2.8.8",
        "prettier-plugin-tailwindcss": "^0.2.8",
        "tailwindcss": "^3.3.2",
        "typescript": "^5.1.6",
        "vite": "^4.3.9",
        "vite-plugin-watch-and-run": "^1.1.3",
        "vue": "^3.3.4",
        "vue-tsc": "^1.8.3"
    },
    "lint-staged": {
        "*": "pnpm lint"
    },
    "dependencies": {
        "@headlessui/vue": "^1.7.14",
        "@heroicons/vue": "1.0.6",
        "maplibre-gl": "^3.1.0",
        "vite-svg-loader": "^4.0.0"
    }
}
